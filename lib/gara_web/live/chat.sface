<div id="main-content" class="content" phx-hook="Main">
    {#for type <- Map.keys(@messages)}
    <div role="alert" class={"alert", alert_class(type)} phx-value-key={type}
	 phx-click="lv:clear-flash">
	{@messages[type]}
    </div>
    {/for}
    <div class="header">
    </div>
    <#slot />
</div>
